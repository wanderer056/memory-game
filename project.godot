;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Memory Game"
run/main_scene="res://scenes/splash_screen.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.png"

[autoload]

GameManager="*res://scenes/GameManager.gd"
SceneTransitioner="*res://scenes/scene_transitioner.tscn"
AudioManager="*res://audio_manager.tscn"

[display]

window/size/viewport_width=1200
window/size/viewport_height=1200
window/stretch/mode="canvas_items"
